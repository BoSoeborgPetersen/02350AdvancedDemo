<Window x:Class="_02350AdvancedDemo.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ViewModel="clr-namespace:_02350AdvancedDemo.ViewModel"
        xmlns:View="clr-namespace:_02350AdvancedDemo.View"
        Title="02350 Advanced Demo" Height="600" Width="800">
    <Window.DataContext>
        <ViewModel:MainViewModel />
    </Window.DataContext>
    <DockPanel>
        <View:Menu DockPanel.Dock="Top"/>
        <View:SidePanelUserControl/>
        <Grid>
            <ItemsControl ItemsSource="{Binding Lines}"/>
            <ItemsControl ItemsSource="{Binding Shapes}"/>
        </Grid>
    </DockPanel>
</Window>
